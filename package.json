  },
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@jest/globals": "^29.5.0",
    "@shelf/jest-mongodb": "^4.1.7",
    "@swc/core": "^1.3.53",
    "@types/chai": "^4.3.4",
    "@types/cors": "^2.8.13",
